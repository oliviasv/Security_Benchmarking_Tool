<if>
  <condition type:"OR">
    <custom_item>
      type        : REG_CHECK
      description : "MySQL 5.6 Community Edition is installed"
      value_type  : POLICY_TEXT
      value_data  : "HKLM\Software\Mysql Ab\Mysql Server 5.6"
      reg_option  : MUST_EXIST
      key_item    : "Version"
