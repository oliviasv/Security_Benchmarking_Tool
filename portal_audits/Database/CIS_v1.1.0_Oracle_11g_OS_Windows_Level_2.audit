  value_data  : "C:\\u01\\app\\oracle\\product\\11.2.0\\dbhome_1\dbs\init.ora"
  check_type  : CHECK_REGEX
  regex       : "^[\s]*[Ss][Qq][Ll]92_[Ss][Ee][Cc][Uu][Rr][Ii][Tt][Yy][\s]*="
