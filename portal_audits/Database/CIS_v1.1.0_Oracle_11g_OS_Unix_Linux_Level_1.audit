  expect      : ""
  reference   : "800-171|3.4.2,800-53|CM-6,CN-L3|8.1.10.6(d),CSCv6|3.1,CSF|PR.IP-1,ITSG-33|CM-6,NESA|T3.2.1,PCI-DSSv3.1|2.2.4,PCI-DSSv3.2|2.2.4,SWIFT-CSCv1|2.3"
</custom_item>

<custom_item>
  type        : FILE_CONTENT_CHECK
  description : "3.04 Oracle account .profile file - 'Unix systems umask 022'"
  info        : "Regardless of where the umask is set, umask must be set to 022 before you install Oracle."
  info        : "An improper umask can lead to unrestrictive permissions and open the Oracle server file system to attack."
  info        : "Level 1, Scorable"
  info        : "NOTE: You may also check for the umask value utilizing the 'umask' bash or sh builtin command."
  see_also    : "https://workbench.cisecurity.org/files/580"
  file        : "/etc/profile"
  regex       : "^[\\s]*umask"
  expect      : "^[\\s]*umask[\\s]*022"
  reference   : "800-171|3.1.1,800-53|AC-3(4),CSF|PR.AC-4,CSF|PR.PT-3,ITSG-33|AC-3(4)"
</custom_item>

