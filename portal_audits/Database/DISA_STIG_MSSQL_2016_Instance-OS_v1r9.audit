  value_type      : POLICY_TEXT
  value_data      : ".*\\Administrators:\(I\)\(F\).*"
  powershell_args : "$f=$env:programfiles + '\Microsoft SQL Server\MSSQL13.MSSQLSERVER\MSSQL\Backup';icacls $f;"
  check_type      : CHECK_REGEX
  severity        : MEDIUM
