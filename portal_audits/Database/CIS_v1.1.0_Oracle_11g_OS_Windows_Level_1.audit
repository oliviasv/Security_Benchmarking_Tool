#
## 1. Operating System Specific Settings
#

<custom_item>
  type          : WMI_POLICY
  description   : "1.01 Windows platform - 'Do not install Oracle on a domain controller'"
  info          : "This action will reduce the attack surface of the Domain Controller and the Oracle server."
  info          : "Level 1, Scorable"
  see_also      : "https://workbench.cisecurity.org/files/580"
  wmi_namespace : "root/CIMV2"
  wmi_request   : "select DomainRole from Win32_ComputerSystem"
  wmi_attribute : "DomainRole"
  wmi_key       : "DomainRole"
  value_type    : POLICY_DWORD
  value_data    : "4" || "5"
  check_type    : CHECK_NOT_EQUAL
  reference     : "800-171|3.4.6,800-171|3.4.7,800-53|CM-7,CN-L3|7.1.3.5(c),CN-L3|8.1.4.4(a),CSF|PR.IP-1,CSF|PR.PT-3,ITSG-33|CM-7,NIAv2|SS15a,SWIFT-CSCv1|2.3"
</custom_item>

<custom_item>
  type          : WMI_POLICY
  description   : "1.02 Windows Oracle Local Account - 'Use Restricted Service Account (RSA)'"
  info          : "Adding an additional administrative account to the system increases the attack surface of the Windows server. Creating a local administrator"
  info          : "with restricted access mitigates this risk."
  info          : "Level 1, Not Scorable"
  info          : "Review the list of Oracle service accounts."
  see_also      : "https://workbench.cisecurity.org/files/580"
  value_type    : POLICY_TEXT
  wmi_namespace : "root/cimv2"
  wmi_request   : "SELECT DisplayName,StartName FROM Win32_Service WHERE (DisplayName like 'OracleService%')"
  wmi_attribute : "StartName"
  wmi_key       : "DisplayName"
  value_data    : ""
  reference     : "800-171|3.1.4,800-171|3.1.5,800-53|AC-5,800-53|AC-6,CN-L3|7.1.3.2(b),CN-L3|7.1.3.2(g),CN-L3|8.1.10.6(a),CN-L3|8.1.4.2(d),CSF|PR.AC-4,CSF|PR.DS-5,ISO/IEC-27001|A.6.1.2,ITSG-33|AC-5,ITSG-33|AC-6,NESA|T5.1.1,NESA|T5.2.2,NESA|T5.4.1,NESA|T5.4.4,NESA|T5.4.5,NESA|T5.5.4,NESA|T5.6.1,NESA|T7.5.3,NIAv2|AM1,NIAv2|AM23f,NIAv2|SS13c,NIAv2|SS15c,PCI-DSSv3.1|7.1.2,PCI-DSSv3.2|7.1.2,SWIFT-CSCv1|5.1,TBA-FIISB|31.4.2,TBA-FIISB|31.4.3"
</custom_item>

<custom_item>
  type        : GROUP_MEMBERS_POLICY
  description : "1.03 Windows Oracle Domain Account - 'Use Restricted Service Account (RSA)'"
  info        : "If the Oracle services require domain resources, then the server must be a domain server and the Oracle services must be run using a"
  info        : "Restricted Service Account, i.e., restricted domain user account."
  info        : "Level 1, Not Scorable"
  see_also    : "https://workbench.cisecurity.org/files/580"
  value_type  : POLICY_TEXT
  group_name  : "Administrators"
# Note: Variable @ORACLE_RSA_ACCOUNT@ replaced with "oracle" in field "value_data".
  value_data  : "oracle"
  reference   : "800-171|3.1.5,800-53|AC-6,CN-L3|7.1.3.2(b),CN-L3|7.1.3.2(g),CN-L3|8.1.10.6(a),CN-L3|8.1.4.2(d),CSF|PR.AC-4,CSF|PR.DS-5,ITSG-33|AC-6,NESA|T5.1.1,NESA|T5.2.2,NESA|T5.4.1,NESA|T5.4.4,NESA|T5.4.5,NESA|T5.5.4,NESA|T5.6.1,NESA|T7.5.3,NIAv2|AM1,NIAv2|AM23f,NIAv2|SS13c,NIAv2|SS15c,PCI-DSSv3.1|7.1.2,PCI-DSSv3.2|7.1.2,SWIFT-CSCv1|5.1,TBA-FIISB|31.4.2,TBA-FIISB|31.4.3"
</custom_item>

<custom_item>
  type        : USER_RIGHTS_POLICY
  description : "1.04 Windows Oracle Account - 'Deny Log on Locally Right'"
  info        : "The RSA must have limited access requirements."
  info        : "Level 1, Scorable"
  see_also    : "https://workbench.cisecurity.org/files/580"
  value_type  : USER_RIGHT
  right_type  : SeDenyInteractiveLogonRight
