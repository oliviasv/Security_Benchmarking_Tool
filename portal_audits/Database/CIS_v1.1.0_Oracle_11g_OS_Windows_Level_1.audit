  value_data  : "oracle"
  reference   : "800-53|AC-6(7),CSCv6|16,CSF|PR.AC-4,ISO/IEC-27001|A.9.2.5,NESA|M1.1.3,NESA|T5.1.1"
</custom_item>

<custom_item>
  type            : AUDIT_POWERSHELL
  description     : "1.05 Windows Oracle Domain Global Group - 'Create a global group for the RSA and make it the RSA's primary group'"
  info            : "The RSA should not have access to resources that all domain users need to access."
  info            : "Level 1, Not Scorable"
  see_also        : "https://workbench.cisecurity.org/files/580"
  value_type      : POLICY_TEXT
# Note: Variable @ORACLE_RSA_ACCOUNT@ replaced with "oracle" in field "powershell_args".
# Note: Variable @RSA_GROUP@ replaced with "RSA_GROUP" in field "powershell_args".
