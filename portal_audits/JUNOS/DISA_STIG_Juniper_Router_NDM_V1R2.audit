  see_also    : "https://dl.dod.cyber.mil/wp-content/uploads/stigs/zip/U_Juniper_Router_V1R2_STIG.zip"
  regex       : "set system login password .+"
  expect      : "set system login password minimum-length 1[5-9][0-9]*"
</custom_item>

