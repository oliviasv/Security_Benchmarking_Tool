  see_also    : "https://dl.dod.cyber.mil/wp-content/uploads/stigs/zip/U_Juniper_Router_V1R2_STIG.zip"
  regex       : "set system login .+"
  expect      : "set system login retry-options tries-before-disconnect 3$"
</custom_item>

