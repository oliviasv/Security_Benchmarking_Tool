<check_type:"Unix">

<custom_item>
#System		: "Solaris"
  type        : FILE_CONTENT_CHECK
  description : "1.2 Install TCP Wrappers - Allow localhost. Note: Replace 172.16.100.0/255.255.255.0 with a network block in use at your organization."
  info        : "TCP Wrappers allow the administrator to control who has access to various network services based on the IP address of the remote end of the connection. TCP Wrappers also provide logging information via syslog about both successful and unsuccessful connections."
  reference   : "800-171|3.13.1,800-171|3.13.6,800-53|SC-7(5),CN-L3|7.1.2.2(c),CSF|PR.PT-4,ITSG-33|SC-7(5),NIAv2|GS7b,NIAv2|NS25"
  see_also    : "https://workbench.cisecurity.org/files/633"
  file        : "/etc/hosts.allow"
  regex       : "^[\\s\\t]*ALL.*"
  expect      : "ALL: *localhost, *172.16.100.0/255.255.255.0"
</custom_item>

<custom_item>
#System		: "Solaris"
  type        : FILE_CONTENT_CHECK
  description : "1.2 Install TCP Wrappers - Deny access to this server from all networks"
  reference   : "800-171|3.13.1,800-171|3.13.6,800-53|SC-7(5),CN-L3|7.1.2.2(c),CSF|PR.PT-4,ITSG-33|SC-7(5),NIAv2|GS7b,NIAv2|NS25"
  see_also    : "https://workbench.cisecurity.org/files/633"
  file        : "/etc/hosts.deny"
  regex       : "^[\\s\\t]*ALL.*"
  expect      : "ALL: ALL"
</custom_item>

<custom_item>
