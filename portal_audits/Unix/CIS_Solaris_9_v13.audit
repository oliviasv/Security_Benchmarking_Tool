  file        : "/etc/vfstab"
  regex       : "^[\\s\\t]*[^ ]+[\\s\\t]*[^ ]+[\\s\\t]+[^ ]+[\\s\\t]+ufs[\\s\\t]+[^ ]+[\\s\\t]+"
  expect      : "^[\\s\\t]*[^ ]+[\\s\\t]*[^ ]+[\\s\\t]+[^ ]+[\\s\\t]+ufs[\\s\\t]+[^ ]+[\\s\\t]+[^ ]+[\\s\\t]+logging"
