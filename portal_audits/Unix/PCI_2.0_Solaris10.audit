<check_type:"Unix">

<custom_item>
  type        : SVC_PROP
  description : "PCI 1.2 Firewall/router configuration restrict connections between untrusted networks and cardholder data environment. - tcp_wrappers enable"
  see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
  reference   : "800-171|3.4.6,800-171|3.4.7,800-53|CM-7,CIP|007-6-R1,CN-L3|7.1.3.5(c),CN-L3|7.1.3.7(d),CN-L3|8.1.4.4(b),CSCv6|9.1,CSF|PR.IP-1,CSF|PR.PT-3,ITSG-33|CM-7,NIAv2|SS13b,NIAv2|SS14a,NIAv2|SS14c,NIAv2|SS15a,PCI-DSS-2.0|1.2,PCI-DSSv3.1|2.2.2,PCI-DSSv3.1|2.2.3,PCI-DSSv3.2|2.2.2,PCI-DSSv3.2|2.2.3,SWIFT-CSCv1|2.3"
  service     : "inetd"
  property    : "defaults/tcp_wrappers"
  value       : "true"
</custom_item>

<custom_item>
  type        : SVC_PROP
  description : "PCI 1.2 Firewall/router configuration restrict connections between untrusted networks and cardholder data environment. - ipfilter enable"
  see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
  reference   : "800-171|3.4.6,800-171|3.4.7,800-53|CM-7,CIP|007-6-R1,CN-L3|7.1.3.5(c),CN-L3|7.1.3.7(d),CN-L3|8.1.4.4(b),CSCv6|9.1,CSF|PR.IP-1,CSF|PR.PT-3,ITSG-33|CM-7,NIAv2|SS13b,NIAv2|SS14a,NIAv2|SS14c,NIAv2|SS15a,PCI-DSS-2.0|1.2,PCI-DSSv3.1|2.2.2,PCI-DSSv3.1|2.2.3,PCI-DSSv3.2|2.2.2,PCI-DSSv3.2|2.2.3,SWIFT-CSCv1|2.3"
  service     : "network/ipfilter:default"
  property    : "general/enabled"
  value       : "true"
</custom_item>

<custom_item>
  system      : "SunOS"
  type        : FILE_CONTENT_CHECK
  description : "PCI 1.2 Firewall/router configuration restrict connections between untrusted networks and cardholder data environment. - TCP Wrappers"
  info        : "In almost every case, it would be wise to enable and configure a firewall. With regards to TCP Wrappers they may be used together for added"
  info        : "security, permitting several different actions to take place depending on connection, reply to services, etc."
  see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
  reference   : "800-171|3.13.1,800-171|3.13.6,800-53|SC-7(5),CN-L3|7.1.2.2(c),CSF|PR.PT-4,ITSG-33|SC-7(5),NIAv2|GS7b,NIAv2|NS25,PCI-DSS-2.0|1.2"
  file        : "/etc/hosts.deny"
  regex       : "[#]*.*ALL.*"
  expect      : "^[^#]*[ ]*ALL: ALL"
</custom_item>

<custom_item>
  system      : "SunOS"
  type        : FILE_CONTENT_CHECK
  description : "PCI 1.2.1/1.3/1.3.2 Restrict inbound and outbound traffic to that which is necessary for the cardholder data environment. - /etc/hosts.allow"
  info        : "NOTE: Replace 192.168. with a network block in use at your organization."
  info        : "1.3 Prohibit direct public access between the Internet and any system component in the cardholder data environment."
  info        : "1.3.2 Limit inbound Internet traffic to IP addresses within the DMZ."
  see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
  reference   : "800-171|3.13.1,800-171|3.13.6,800-53|SC-7(5),CN-L3|7.1.2.2(c),CSF|PR.PT-4,ITSG-33|SC-7(5),NIAv2|GS7b,NIAv2|NS25,PCI-DSS-2.0|1.2.1,PCI-DSS-2.0|1.3,PCI-DSS-2.0|1.3.2"
  file        : "/etc/hosts.allow"
  regex       : "[#]*.*ALL.*"
# Note: Variable @HOSTS_ALLOW_NETWORK@ replaced with "192.168.0.0/255.255.255.0" in field "expect".
  expect      : "^[^#]*[ ]*ALL: localhost,192.168.0.0/255.255.255.0"
</custom_item>

<custom_item>
  system        : "SunOS"
  type          : CMD_EXEC
  description   : "PCI 1.2.1/1.3/1.3.2 Restrict inbound and outbound traffic to that which is necessary for the cardholder data environment. - 'ipfilter rules'"
  info          : "NOTE: Replace 192.168. with a network block in use at your organization."
  info          : "1.3 Prohibit direct public access between the Internet and any system component in the cardholder data environment."
  info          : "1.3.2 Limit inbound Internet traffic to IP addresses within the DMZ."
  info          : "NOTE: Review IP Filter rules to ensure they are properly filtering traffic."
  see_also      : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
  reference     : "800-171|3.13.1,800-53|SC-7(12),CSCv6|9.2,ITSG-33|SC-7(12),NIAv2|AM38,NIAv2|SS13d,NIAv2|SS26,PCI-DSS-2.0|1.2.1,PCI-DSS-2.0|1.3,PCI-DSS-2.0|1.3.2"
  cmd           : "/usr/bin/pfexec ipfstat -ni"
  dont_echo_cmd : NO
  quiet         : NO
  expect        : "[^$]"
</custom_item>

<if>
  <condition type:"AND">
    <custom_item>
      system      : "SunOS"
      type        : PKG_CHECK
      description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have personal firewall installed. - 'SUNWipfr'"
      pkg         : "SUNWipfr"
    </custom_item>

    <custom_item>
      system      : "SunOS"
      type        : PKG_CHECK
      description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have personal firewall installed. - 'SUNWipfu'"
      pkg         : "SUNWipfu"
    </custom_item>

    <custom_item>
      system      : "SunOS"
      type        : PKG_CHECK
      description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have personal firewall installed. - 'SUNWipfh'"
      pkg         : "SUNWipfh"
    </custom_item>
  </condition>

  <then>
    <report type:"PASSED">
      description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have personal firewall installed - 'IP Filter'"
      see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
    </report>
  </then>

  <else>
    <report type:"FAILED">
      description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have personal firewall installed - 'IP Filter'"
      see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
      info        : "Note: Nessus did not find evidence of IP Filter installed."
    </report>
  </else>
</if>

