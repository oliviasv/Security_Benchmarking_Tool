      reference   : "LEVEL|1S"
      see_also    : "https://workbench.cisecurity.org/files/268"
    </report>

    <report type:"PASSED">
      description : "2.1.6 Create administrator accounts with difficult-to-guess names"
      info        : "Ensure administrator accounts have unique names that are not easily-guessable, such as 'admin' or 'administrator'."
      info        : "Note: Nessus has not performed this check since the check is not scorable. Please manually review the CIS reference and make sure the system is compliant."
