<check_type:"Unix">

<custom_item>
  system      : "Linux"
  type        : FILE_CONTENT_CHECK
  description : "BSI-100-2: S 2.33: Division of administrator roles under Unix: Password length >= 12"
  info        : "The password must have an extended minimum length (12 characters or more)

Safeguard Catalogues: S 2: Organisation

S 2.33: Division of administrator roles under Unix"
  reference   : "800-171|3.5.7,800-53|IA-5(1),BSI-100-2|S2.33,CIP|007-6-R5,CN-L3|7.1.2.7(e),CN-L3|7.1.3.1(b),CSF|PR.AC-1,HIPAA|164.308(a)(5)(ii)(D),ISO/IEC-27001|A.9.4.3,ITSG-33|IA-5(1),NESA|T5.2.3,NIAv2|AM19a,NIAv2|AM19b,NIAv2|AM19c,NIAv2|AM19d,NIAv2|AM22a,PCI-DSSv3.1|8.2.3,PCI-DSSv3.2|8.2.3,PCI|8.5.10,SWIFT-CSCv1|4.1,TBA-FIISB|26.2.1,TBA-FIISB|26.2.4"
  see_also    : "https://www.bsi.bund.de/cae/servlet/contentblob/471430/publicationFile/28223/standard_100-2_e_pdf.pdf"
  file        : "/etc/pam.d/system-auth"
  regex       : "^[\\s]*password\\s+required\\s+pam_cracklib.so\\s.*\\s?minlen="
  expect      : "^[\\s]*password\\s+required\\s+pam_cracklib.so\\s(.*\\s)?minlen=(1[2-9]|[2-9][0-9])"
</custom_item>

