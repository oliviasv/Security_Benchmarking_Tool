<check_type:"Unix">

<if>
  <condition type:"AND">
    <custom_item>
      system      : "AIX"
      type        : CMD_EXEC
      description : "AIX 5.3 or 6.1 is installed"
      cmd         : "/usr/bin/oslevel -s"
      expect      : "^(5300|6100)"
    </custom_item>
  </condition>

  <then>
    <report type:"PASSED">
      description : "CIS AIX 5.3 and 6.1 v1.1.0 Level 2"
    </report>

##
## 1. AIX Security Expert Recommendations
##
###
### 1.1 AIX Security Expert - Password Policy
###

    <if>
      <condition type:"OR">
        <custom_item>
          system      : "AIX"
          type        : CMD_EXEC
          description : "1.1.11 - /etc/security/login.cfg - 'pwd_algorithm = ssha256 (AIX 5.3 TL7+ only)'"
          cmd         : "/usr/bin/oslevel -r"
          expect      : "^5300-007$"
        </custom_item>
      </condition>

      <then>
        <custom_item>
          system      : "AIX"
          type        : CMD_EXEC
          description : "1.1.11 - /etc/security/login.cfg - 'pwd_algorithm = ssha256 (AIX 5.3 TL7+ only)'"
          info        : "Checks the current pwd_algorithm setting."
          reference   : "800-171|3.5.2,800-53|IA-5,800-53|SI-7(6),CSF|PR.AC-1,CSF|PR.DS-6,ITSG-33|IA-5,LEVEL|2S,NESA|T5.2.3,SWIFT-CSCv1|6.2"
          see_also    : "https://workbench.cisecurity.org/files/525"
          cmd         : "/usr/bin/lssec -f /etc/security/login.cfg -s usw -a pwd_algorithm"
          expect      : "^usw[\\s]+pwd_algorithm[\\s]*=[\\s]*ssha256$"
        </custom_item>
      </then>

      <else>
        <report type:"PASSED">
          description : "1.1.11 - /etc/security/login.cfg - 'pwd_algorithm = ssha256 (AIX 5.3 TL7+ only)'"
          info        : "This system is not running AIX 5.3 TL7 or higher."
          reference   : "800-171|3.5.2,800-53|IA-5,800-53|SI-7,CSF|PR.AC-1,CSF|PR.DS-6,ITSG-33|IA-5,LEVEL|2S,NESA|T5.2.3,SWIFT-CSCv1|6.2"
        </report>
      </else>
    </if>

