<check_type:"Unix">

<if>
  <condition type:"OR">
    <custom_item>
      system      : "Linux"
      type        : FILE_CONTENT_CHECK
      description : "Red Hat Enterprise Linux is installed"
      file        : "/etc/redhat-release"
      regex       : "^[\\s]*Red Hat Enterprise Linux (Server|Workstation)"
      expect      : "^[\\s]*Red Hat Enterprise Linux (Server|Workstation)"
    </custom_item>

    <custom_item>
      system      : "Linux"
      type        : FILE_CONTENT_CHECK
      description : "CentOS Linux is installed"
      file        : "/etc/centos-release"
      regex       : "^[\\s]*CentOS"
      expect      : "^[\\s]*CentOS"
    </custom_item>
  </condition>

  <then>
    <report type:"PASSED">
      description : "PCI_2.0_Redhat.audit"
    </report>

    <custom_item>
      system      : "Linux"
      type        : CMD_EXEC
      description : "PCI 1.2 Build firewall and router configurations that restrict connections between untrusted networks and cardholder data environment"
      info        : "Review iptables config, make sure unauthorized connections are not allowed.

NOTE: Nessus has provided the target output to assist in reviewing the benchmark to ensure target compliance."
      reference   : "800-171|3.13.1,800-53|SC-7(12),CSCv6|9.2,ITSG-33|SC-7(12),NIAv2|AM38,NIAv2|SS13d,NIAv2|SS26,PCI-DSS-2.0|1.2"
      see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
      cmd         : "/sbin/iptables -nL --line-numbers"
      expect      : ""
      severity    : MEDIUM
    </custom_item>

    <custom_item>
      system      : "Linux"
      type        : FILE_CONTENT_CHECK
      description : "PCI 1.2 Build firewall and router configurations that restrict connections between untrusted networks"
      reference   : "800-171|3.13.1,800-171|3.13.6,800-53|SC-7(5),CN-L3|7.1.2.2(c),CSF|PR.PT-4,ITSG-33|SC-7(5),NIAv2|GS7b,NIAv2|NS25,PCI-DSS-2.0|1.2"
      see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
      file        : "/etc/hosts.deny"
      regex       : "^[\\s\\t]*ALL[\\s\\t]*:"
      expect      : "^[\\s\\t]*ALL[\\s\\t]*:[\\s\\t]*ALL[\\s\\t]*$"
    </custom_item>

    <custom_item>
# Note: Variable @HOSTS_ALLOW_NETWORK@ replaced with "192\\.168\\." in field "#expect".
#expect	 : "^[\\s\\t]*ALL[\\s\\t]*:[\\s\\t]localhost,[\\s\\t]+192\\.168\\."
#expect	 : "^[\\s\\t]*ALL[\\s\\t]*:[\\s\\t]localhost,[\\s\\t]+192\.168\."
      system      : "Linux"
      type        : FILE_CONTENT_CHECK
      description : "PCI 1.2.1/1.3/1.3.2 Restrict inbound and outbound traffic to that which is necessary for the cardholder data environment"
      info        : "PCI 1.3 Prohibit direct public access between the Internet and any system component in the cardholder data environment."
      info        : "PCI 1.3.2 Limit inbound Internet traffic to IP addresses within the DMZ."
      reference   : "800-171|3.13.1,800-171|3.13.6,800-53|SC-7(5),CN-L3|7.1.2.2(c),CSF|PR.PT-4,ITSG-33|SC-7(5),NIAv2|GS7b,NIAv2|NS25,PCI-DSS-2.0|1.2.1,PCI-DSS-2.0|1.3,PCI-DSS-2.0|1.3.2"
      see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
      file        : "/etc/hosts.allow"
      regex       : "^[\\s\\t]*ALL[\\s\\t]*:"
# Note: Variable @INTERNAL_NETWORK@ replaced with "192\\.168\\." in field "expect".
      expect      : "^[\\s\\t]*ALL[\\s\\t]*:[\\s\\t]localhost,[\\s\\t]+192\\.168\\."
    </custom_item>

    <if>
      <condition type:"OR">
        <custom_item>
          type        : RPM_CHECK
          description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have personal firewall installed"
          rpm         : "McAfeeVSEForLinux-0.0.0-0"
          operator    : "gt"
        </custom_item>

        <custom_item>
          type        : RPM_CHECK
          description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have personal firewall installed"
          rpm         : "kav4ws-0.0.0-0"
          operator    : "gt"
        </custom_item>

        <custom_item>
          type        : RPM_CHECK
          description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have personal firewall installed"
          rpm         : "LinuxShield-0.0.0-0"
          operator    : "gt"
        </custom_item>

        <custom_item>
          type        : CHKCONFIG
          description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have personal firewall installed"
          service     : "iptables"
          levels      : "35"
          status      : ON
        </custom_item>
      </condition>

      <then>
        <report type:"PASSED">
          description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have personal firewall installed"
          reference   : "PCI-DSS-2.0|1.4"
          see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
        </report>
      </then>

      <else>
        <report type:"FAILED">
          description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have personal firewall installed"
          info        : "Note: Nessus did not find evidence of a firewall software installed."
          info        : "If a firewall is installed, please edit this check to look for the installed firewall."
          reference   : "PCI-DSS-2.0|1.4"
          see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
        </report>
      </else>
    </if>

    <report type:"WARNING">
      description : "PCI 2.2 Develop configuration standards for all system components"
      info        : "Consider running a Nessus compliance scan for Red Hat OS using Tenable .audit files that have received CIS certifications.

NOTE: Nessus has not performed this check. Please review the benchmark to ensure target compliance."
      reference   : "PCI-DSS-2.0|2.2"
      see_also    : "https://www.pcisecuritystandards.org/documents/PCI_DSS_v3.pdf"
    </report>

    <custom_item>
