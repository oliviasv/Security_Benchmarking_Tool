<check_type:"Windows" version:"2">
<group_policy:"Microsoft Security Compliance Toolkit">

<if>
  <condition type:"AND">
    <custom_item>
      type        : REGISTRY_SETTING
      description : "Check Product Name"
      value_type  : POLICY_TEXT
      value_data  : "Windows[\s]+Server"
      reg_key     : "HKLM\Software\Microsoft\Windows Nt\Currentversion"
      reg_item    : "ProductName"
      check_type  : CHECK_REGEX
    </custom_item>

    <custom_item>
      type        : REGISTRY_SETTING
      description : "Check Release ID"
      value_type  : POLICY_TEXT
      value_data  : "1903"
      reg_key     : "HKLM\Software\Microsoft\Windows Nt\Currentversion"
      reg_item    : "ReleaseID"
      check_type  : CHECK_REGEX
    </custom_item>

    <custom_item>
      type          : WMI_POLICY
      description   : "Check if server is Domain Controller"
      value_type    : POLICY_DWORD
      value_data    : 4 || 5
      wmi_namespace : "root/CIMV2"
      wmi_request   : "select DomainRole from Win32_ComputerSystem"
      wmi_attribute : "DomainRole"
      wmi_key       : "DomainRole"
    </custom_item>
  </condition>

  <then>
    <report type:"PASSED">
      description : "MSCT_Windows_Server_1903_DC_v1.19.9.audit from MSCT Windows 10 Version 1903 and Windows Server Version 1903 Security Baseline"
    </report>

    <custom_item>
      type        : USER_RIGHTS_POLICY
