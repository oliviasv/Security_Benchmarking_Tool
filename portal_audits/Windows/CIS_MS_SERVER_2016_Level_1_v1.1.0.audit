      reference   : "800-171|3.13.8,800-53|SC-8,CCE|CCE-37553-5,CN-L3|8.1.2.2(a),CN-L3|8.1.2.2(b),CN-L3|8.1.4.7(a),CN-L3|8.1.4.8(a),CN-L3|8.2.4.5(c),CN-L3|8.2.4.5(d),CN-L3|8.5.2.2,CSCv6|13,CSF|PR.DS-2,CSF|PR.DS-5,ITSG-33|SC-8,LEVEL|1S,NESA|T4.3.1,NESA|T4.3.2,NESA|T4.5.1,NESA|T4.5.2,NESA|T7.3.3,NESA|T7.4.1,NIAv2|IE12,NIAv2|IE8,NIAv2|IE9,NIAv2|NS29,NIAv2|SS24"
      see_also    : "https://workbench.cisecurity.org/files/2163"
      value_type  : POLICY_DWORD
      value_data  : 537395200
      reg_key     : "HKLM\System\Currentcontrolset\Control\Lsa\Msv1_0"
      reg_item    : "NTLMMinClientSec"
      reg_option  : CAN_NOT_BE_NULL
    </custom_item>

