      value_type  : POLICY_DWORD
      value_data  : [32768..MAX]
      reg_key     : "HKLM\Software\Policies\Microsoft\Windows\Eventlog\Application"
      reg_item    : "MaxSize"
      reg_option  : CAN_NOT_BE_NULL
    </custom_item>

