#<ui_metadata>
#<display_name>DISA STIG Office 2007 Outlook v4r9</display_name>
#<spec>
#  <type>DISA STIG</type>
#  <name>Office 2007 Outlook</name>
#  <version>4.9.0</version>
#</spec>
#<labels>windows,microsoft,office,office_2007,access,access_2007</labels>
