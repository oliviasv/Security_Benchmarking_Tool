      value_data           : "-1"
      reg_option           : CAN_BE_NULL
      reg_ignore_hku_users : "S-1-5-18,S-1-5-19,S-1-5-20"
    </custom_item>

    <custom_item>
      type                 : REGISTRY_SETTING
      description          : "DTOO236 - Outlook - All installed trusted COM addins can be trusted."
      info                 : "All installed trusted COM addins can be trusted.  Because the add-ins are controlled and known by the admins, they should always be presumed trusted."
      solution             : "The policy value for User Configuration -> Administrative Templates -> Microsoft Office Outlook 2007 -> Security 'Configure Add-In Trust Level' will be set to 'Enabled (Trust all loaded and installed COM addins)'."
      reference            : "800-171|3.13.13,800-53|SC-18(4),CAT|II,CSF|DE.CM-5,ITSG-33|SC-18(4),Rule-ID|SV-18671r2_rule,STIG-ID|DTOO236,Vuln-ID|V-17566"
