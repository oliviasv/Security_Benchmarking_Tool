      value_data  : "0"
      reg_key     : "HKLM\SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System\CredSSP\Parameters"
      reg_item    : "AllowEncryptionOracle"
      reg_option  : CAN_NOT_BE_NULL
    </custom_item>

