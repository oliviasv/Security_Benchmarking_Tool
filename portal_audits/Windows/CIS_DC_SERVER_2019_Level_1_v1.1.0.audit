      value_type  : POLICY_DWORD
      value_data  : "0" || "1"
      reg_key     : "HKLM\Software\Policies\Microsoft\Windows\DataCollection"
      reg_item    : "AllowTelemetry"
      reg_option  : CAN_NOT_BE_NULL
    </custom_item>

