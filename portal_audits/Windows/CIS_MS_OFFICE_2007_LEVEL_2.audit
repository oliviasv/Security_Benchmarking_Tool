<check_type:"Windows" version:"2">
<group_policy:"CIS MS Office 2007: Level II">

<if>
  <condition type:"AND">
    <custom_item>
      type        : REGISTRY_SETTING
      description : "Office 2007 is installed"
      value_type  : POLICY_DWORD
      value_data  : [1..MAX]
      reg_key     : "HKLM\SOFTWARE\Microsoft\Office\12.0\Common\InstallRoot"
      reg_item    : "InstallCount"
      reg_option  : CAN_NOT_BE_NULL
    </custom_item>
  </condition>

  <then>
    <report type:"PASSED">
      description : "CIS_MS_OFFICE_2007_LEVEL_2.audit Level 2"
    </report>

# 1.1.1.2. Office Updates: Level II

    <custom_item>
      type                 : REGISTRY_SETTING
      description          : "1.1.1.2. Office Updates: Level II Enabled"
      info                 : "The Block updates from the Office Update Site from applying option determines if a User may acquire updates from the Microsoft Office Update site, the Check for Updates menu and task pane items."
      solution             : "Make sure 'Office Updates: Level II' is Enabled."
      reference            : "800-171|3.4.6,800-171|3.4.7,800-53|CM-7,CCE|CCE-784-9,CN-L3|7.1.3.5(c),CN-L3|7.1.3.7(d),CN-L3|8.1.4.4(b),CSF|PR.IP-1,CSF|PR.PT-3,ITSG-33|CM-7,LEVEL|2S,NIAv2|SS13b,NIAv2|SS14a,NIAv2|SS14c,NIAv2|SS15a,PCI-DSS|2.2.4,PCI-DSS|6.1,SWIFT-CSCv1|2.3"
      see_also             : "https://workbench.cisecurity.org/files/618"
      value_type           : POLICY_DWORD
      value_data           : 1
      reg_key              : "HKU\Software\Policies\Microsoft\Office\Common\OfficeUpdate"
      reg_item             : "BlockUpdates"
      reg_ignore_hku_users : "S-1-5-18,S-1-5-19,S-1-5-20"
      reg_option           : CAN_NOT_BE_NULL
    </custom_item>

# 1.1.1.3. Disable Repairing Corrupt Office Open XML: Level II

    <custom_item>
      type        : REGISTRY_SETTING
      description : "1.1.1.3. Disable Repairing Corrupt Office Open XML: Level II Enabled"
      info        : "Setting the Disable Package Repair option to enabled reduces the attack surface area of the Office application suite."
      solution    : "Make sure 'Disable Repairing Corrupt Office Open XML: Level II' is Enabled."
      reference   : "800-53|CM-6,800-53|SI-2,CCE|CCE-933-2,LEVEL|2S,PCI-DSS|2.2.3"
      see_also    : "https://workbench.cisecurity.org/files/618"
      value_type  : POLICY_DWORD
      value_data  : 1
      reg_key     : "HKLM\Software\Policies\Microsoft\Office\12.0\Common\OpenXMLFormat"
      reg_item    : "DisablePackageRepair"
      reg_option  : CAN_NOT_BE_NULL
    </custom_item>

# 1.1.1.4. Prevent Using Visual Basic for Applications: Level II

    <custom_item>
      type        : REGISTRY_SETTING
      description : "1.1.1.4. Prevent Using Visual Basic for Applications: Level II Enabled"
      info        : "The Disable VBA for Office applications setting will prevent Office applications from using VBA."
      solution    : "Make sure 'Prevent Using Visual Basic for Applications: Level II' is Enabled."
      reference   : "800-53|CM-5,800-53|CM-6,CCE|CCE-116-4,LEVEL|2S,PCI-DSS|2.2.4"
      see_also    : "https://workbench.cisecurity.org/files/618"
      value_type  : POLICY_DWORD
      value_data  : 1
      reg_key     : "HKLM\Software\Policies\Microsoft\Office\12.0\Common"
      reg_item    : "VbaOff"
      reg_option  : CAN_NOT_BE_NULL
    </custom_item>

