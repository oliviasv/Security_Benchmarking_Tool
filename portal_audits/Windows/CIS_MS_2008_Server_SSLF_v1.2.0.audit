<check_type:"Windows" version:"2">
<group_policy:"CIS Security Configuration Benchmark For Microsoft Windows Server 2008">

<if>
  <condition type:"AND">
    <custom_item>
      type        : REGISTRY_SETTING
      description : "Windows Server 2008 is installed"
      value_type  : POLICY_TEXT
      value_data  : "^[a-zA-Z0-9\(\)\s]*2008[a-zA-Z0-9\(\)\s]*$"
      reg_key     : "HKLM\SOFTWARE\Microsoft\Windows NT\CurrentVersion"
      reg_item    : "ProductName"
      check_type  : CHECK_REGEX
    </custom_item>
  </condition>

  <then>
    <report type:"PASSED">
      description : "CIS Security Benchmark For Microsoft Windows Server 2008 SSLF Domain Controller."
    </report>

#
## 1.1 Account Policies
#

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.1 Enforce password history (>=24)"
      info            : "This control defines the number of unique passwords a user must leverage before a previously used password can be reused. If an attacker compromises a given credential that is then expired, this control prevents the user from reusing that same compromised credential."
      solution        : "Make sure 'Enforce password history' is set to remember a minimum of 24 passwords."
      reference       : "800-171|3.5.8,800-53|IA-5(1),CCE|CCE-2237-6,CSF|PR.AC-1,HIPAA|164.308(a)(5)(ii)(D),ISO/IEC-27001|A.9.4.3,ITSG-33|IA-5(1),NESA|T5.2.3,NIAv2|AM22c,PCI-DSSv3.1|8.2.5,PCI-DSSv3.2|8.2.5,PCI-DSS|8.5.12,SWIFT-CSCv1|4.1,TBA-FIISB|26.2.3"
      see_also        : "https://workbench.cisecurity.org/files/10"
      value_type      : POLICY_DWORD
      value_data      : [24..MAX]
      password_policy : ENFORCE_PASSWORD_HISTORY
    </custom_item>

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.2 Maximum password age (Max of 90 days or less)"
      info            : "This control defines how many days a user can use the same password before it expires. Having a shorter password age will decrease the chances of a brute force attack. A brute force attack is one where the attacker guesses every possible character combination by splitting password into smaller made from the original password."
      solution        : "Make sure 'Maximum password age' is set to a maximum of 90 days."
      reference       : "800-171|3.5.10,800-171|3.5.7,800-171|3.5.8,800-171|3.5.9,800-53|IA-5(1),CCE|CCE-2200-4,CIP|007-6-R5,CN-L3|7.1.2.7(e),CN-L3|7.1.3.1(b),CSF|PR.AC-1,HIPAA|164.308(a)(5)(ii)(D),ISO/IEC-27001|A.9.4.3,ITSG-33|IA-5(1),NESA|T5.2.3,NIAv2|AM20,NIAv2|AM21,PCI-DSSv3.1|8.2.4,PCI-DSSv3.2|8.2.4,PCI-DSS|8.5.9,SWIFT-CSCv1|4.1,TBA-FIISB|26.2.2"
      see_also        : "https://workbench.cisecurity.org/files/10"
      value_type      : TIME_DAY
      value_data      : [MIN..90]
      password_policy : MAXIMUM_PASSWORD_AGE
    </custom_item>

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.3 Minimum password age (>=1)"
      info            : "This control defines how many days a user must use the same password before it can be changed. This control should be configured to at least 1 day so a user cannot keep changing his password in order to clear the cache to use the same original password."
      solution        : "Make sure 'Minimum password age' is set to a minimum of 1 day."
      reference       : "800-171|3.5.10,800-171|3.5.7,800-171|3.5.8,800-171|3.5.9,800-53|IA-5(1),CCE|CCE-1861-4,CIP|007-6-R5,CN-L3|7.1.2.7(e),CN-L3|7.1.3.1(b),CSF|PR.AC-1,HIPAA|164.308(a)(5)(ii)(D),ISO/IEC-27001|A.9.4.3,ITSG-33|IA-5(1),NESA|T5.2.3,NIAv2|AM20,NIAv2|AM21,PCI-DSSv3.1|8.2.4,PCI-DSSv3.2|8.2.4,PCI-DSS|8.5,SWIFT-CSCv1|4.1,TBA-FIISB|26.2.2"
      see_also        : "https://workbench.cisecurity.org/files/10"
      value_type      : TIME_DAY
      value_data      : [1..MAX]
      password_policy : MINIMUM_PASSWORD_AGE
    </custom_item>

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.4 Minimum password length (>=12)"
      info            : "This control defines the minimum number of characters a user password must contain. Enforcing a minimum password length helps protect against brute force and dictionary attacks."
      solution        : "Make sure 'Minimum password length' is set to a minimum of 8 characters."
      reference       : "800-171|3.5.7,800-53|IA-5(1),CCE|CCE-2240-0,CIP|007-6-R5,CN-L3|7.1.2.7(e),CN-L3|7.1.3.1(b),CSF|PR.AC-1,HIPAA|164.308(a)(5)(ii)(D),ISO/IEC-27001|A.9.4.3,ITSG-33|IA-5(1),NESA|T5.2.3,NIAv2|AM19a,NIAv2|AM19b,NIAv2|AM19c,NIAv2|AM19d,NIAv2|AM22a,PCI-DSSv3.1|8.2.3,PCI-DSSv3.2|8.2.3,PCI-DSS|8.5.10,SWIFT-CSCv1|4.1,TBA-FIISB|26.2.1,TBA-FIISB|26.2.4"
      see_also        : "https://workbench.cisecurity.org/files/10"
      value_type      : POLICY_DWORD
      value_data      : [12..MAX]
      password_policy : MINIMUM_PASSWORD_LENGTH
    </custom_item>

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.5 Password must meet complexity requirements"
      info            : "This control determines if new passwords are required to satisfy a certain level of complexity."
      solution        : "Make sure 'Password must meet complexity requirements' is Enabled."
      reference       : "800-171|3.5.7,800-53|IA-5(1),CCE|CCE-2126-1,CIP|007-6-R5,CN-L3|7.1.2.7(e),CN-L3|7.1.3.1(b),CSF|PR.AC-1,HIPAA|164.308(a)(5)(ii)(D),ISO/IEC-27001|A.9.4.3,ITSG-33|IA-5(1),NESA|T5.2.3,NIAv2|AM19a,NIAv2|AM19b,NIAv2|AM19c,NIAv2|AM19d,NIAv2|AM22a,PCI-DSSv3.1|8.2.3,PCI-DSSv3.2|8.2.3,PCI-DSS|8.5,SWIFT-CSCv1|4.1,TBA-FIISB|26.2.1,TBA-FIISB|26.2.4"
      see_also        : "https://workbench.cisecurity.org/files/10"
      value_type      : POLICY_SET
      value_data      : "Enabled"
      password_policy : COMPLEXITY_REQUIREMENTS
    </custom_item>

    <custom_item>
      type            : PASSWORD_POLICY
      description     : "1.1.6 Store passwords using reversible encryption"
      info            : "The Windows authentication model allows storage of a password hash rather than the actual password."
      solution        : "Make sure 'Store passwords using reversible encryption' is Disabled."
      reference       : "800-171|3.5.10,800-53|IA-5(1),CCE|CCE-2289-7,CSCv6|16.13,CSCv6|16.14,CSF|PR.AC-1,ITSG-33|IA-5(1),NESA|T5.2.3,NIAv2|CY6,PCI-DSSv3.1|8.2.1,PCI-DSSv3.2|8.2.1,PCI-DSS|8.4,SWIFT-CSCv1|4.1,TBA-FIISB|26.1"
      see_also        : "https://workbench.cisecurity.org/files/10"
      value_type      : POLICY_SET
      value_data      : "Disabled"
      password_policy : REVERSIBLE_ENCRYPTION
    </custom_item>

    <if>
      <condition type:"AND">
        <custom_item>
          type            : AUDIT_POWERSHELL
          description     : "Test the Lockout Threshold"
          value_type      : POLICY_TEXT
          value_data      : "[Ll]ockout threshold:[\s]*[1-9][\d]*"
          powershell_args : "net accounts | findstr /c:'Lockout threshold'"
          check_type      : CHECK_REGEX
        </custom_item>
      </condition>

      <then>
        <custom_item>
          type           : LOCKOUT_POLICY
          description    : "1.1.7 Account lockout duration"
          info           : "This control defines the minimum number of minutes a user must wait before a locked account is unlocked."
          solution       : "Make sure 'Account lockout duration' is set to a minimum of 15 minutes."
          reference      : "800-171|3.1.8,800-53|AC-7,CCE|CCE-1317-7,CN-L3|8.1.4.1(b),CSCv6|16.7,ITSG-33|AC-7,NESA|T5.5.1,NIAv2|AM24,PCI-DSS|8.5.14,TBA-FIISB|36.2.4,TBA-FIISB|45.1.2,TBA-FIISB|45.2.1,TBA-FIISB|45.2.2"
          see_also       : "https://workbench.cisecurity.org/files/10"
          value_type     : TIME_MINUTE
          value_data     : [15..MAX]
          lockout_policy : LOCKOUT_DURATION
        </custom_item>

        <custom_item>
          type           : LOCKOUT_POLICY
          description    : "1.1.8 Account lockout threshold"
          info           : "This control defines the number of failed logon attempts before a user is locked out of an account."
          solution       : "Make sure 'Account lockout threshold' is set to a maximum of 10 attempts."
          reference      : "800-171|3.1.8,800-53|AC-7,CCE|CCE-1872-1,CN-L3|8.1.4.1(b),CSCv6|16.7,ITSG-33|AC-7,NESA|T5.5.1,NIAv2|AM24,PCI-DSS|8.5.13,TBA-FIISB|36.2.4,TBA-FIISB|45.1.2,TBA-FIISB|45.2.1,TBA-FIISB|45.2.2"
          see_also       : "https://workbench.cisecurity.org/files/10"
          value_type     : POLICY_DWORD
          value_data     : [1..10]
          lockout_policy : LOCKOUT_THRESHOLD
        </custom_item>
      </then>

      <else>
        <report type:"FAILED">
          description : "1.1.7 Account lockout duration"
          info        : "This control defines the minimum number of minutes a user must wait before a locked account is unlocked."
          solution    : "Make sure 'Account lockout duration' is set to a minimum of 15 minutes."
          reference   : "800-171|3.1.8,800-53|AC-7,CCE|CCE-1317-7,CN-L3|8.1.4.1(b),CSCv6|16.7,ITSG-33|AC-7,NESA|T5.5.1,NIAv2|AM24,PCI-DSS|8.5.14,TBA-FIISB|36.2.4,TBA-FIISB|45.1.2,TBA-FIISB|45.2.1,TBA-FIISB|45.2.2"
          see_also    : "https://workbench.cisecurity.org/files/10"
        </report>

        <report type:"FAILED">
          description : "1.1.8 Account lockout threshold"
          info        : "This control defines the number of failed logon attempts before a user is locked out of an account."
          solution    : "Make sure 'Account lockout threshold' is set to a maximum of 10 attempts."
          reference   : "800-171|3.1.8,800-53|AC-7,CCE|CCE-1872-1,CN-L3|8.1.4.1(b),CSCv6|16.7,ITSG-33|AC-7,NESA|T5.5.1,NIAv2|AM24,PCI-DSS|8.5.13,TBA-FIISB|36.2.4,TBA-FIISB|45.1.2,TBA-FIISB|45.2.1,TBA-FIISB|45.2.2"
          see_also    : "https://workbench.cisecurity.org/files/10"
        </report>
      </else>
    </if>

    <custom_item>
      type           : LOCKOUT_POLICY
      description    : "1.1.9 Reset account lockout counter after"
      info           : "Following an unsuccessful logon, the system increments the count of invalid attempts for this account."
      solution       : "Make sure 'Reset account lockout counter after' is set to a minimum of 15 minutes."
      reference      : "800-171|3.1.8,800-53|AC-7,CCE|CCE-2311-9,CN-L3|8.1.4.1(b),CSCv6|16.7,ITSG-33|AC-7,NESA|T5.5.1,NIAv2|AM24,PCI-DSS|8.5,TBA-FIISB|36.2.4,TBA-FIISB|45.1.2,TBA-FIISB|45.2.1,TBA-FIISB|45.2.2"
      see_also       : "https://workbench.cisecurity.org/files/10"
      value_type     : TIME_MINUTE
      value_data     : [15..MAX]
      lockout_policy : LOCKOUT_RESET
    </custom_item>

    <if>
      <condition type:"OR">
        <custom_item>
          type        : REGISTRY_SETTING
          description : "Test to determine PDC."
          value_type  : POLICY_TEXT
          value_data  : "LANMANNT"
          reg_key     : "HKLM\System\currentcontrolset\control\ProductOptions"
          reg_item    : "ProductType"
          reg_option  : CAN_NOT_BE_NULL
          check_type  : CHECK_EQUAL
        </custom_item>

        <custom_item>
          type        : REGISTRY_SETTING
          description : "Test to determine DC."
          value_type  : POLICY_TEXT
          value_data  : "lansecnt"
          reg_key     : "HKLM\System\currentcontrolset\control\ProductOptions"
          reg_item    : "ProductType"
          reg_option  : CAN_NOT_BE_NULL
          check_type  : CHECK_EQUAL
        </custom_item>
      </condition>

      <then>
        <custom_item>
          type            : KERBEROS_POLICY
          description     : "1.1.10 Enforce user logon restrictions"
          info            : "This control defines Kerberos-related attributes of domain user accounts, such as the Maximum lifetime for user ticket and Enforce user logon restrictions settings."
          solution        : "Make sure 'Enforce user logon restrictions' is set to a minimum of 15 minutes."
          reference       : "800-53|AC-2(11),CCE|CCE-8594-4,CN-L3|8.1.4.1(b),CSF|DE.CM-1,CSF|DE.CM-3,CSF|PR.AC-1,CSF|PR.AC-4,PCI-DSS|2.2.3"
          see_also        : "https://workbench.cisecurity.org/files/10"
          value_type      : POLICY_SET
          value_data      : "Enabled"
          kerberos_policy : USER_LOGON_RESTRICTIONS
        </custom_item>

        <custom_item>
          type            : KERBEROS_POLICY
          description     : "1.1.11 Maximum tolerance for computer clock synchronization"
          info            : "This control defines maximum tolerance for computer clock synchronization."
          solution        : "Make sure 'Maximum tolerance for computer clock synchronization' is set to a value of 5 minutes."
          reference       : "800-171|3.3.7,800-53|AU-8(1),CCE|CCE-8268-5,CSCv6|6.1,CSF|PR.PT-1,ISO/IEC-27001|A.12.4.4,ITSG-33|AU-8(1),NESA|T3.6.7,NIAv2|NS44,NIAv2|NS45,NIAv2|NS46,NIAv2|NS47,PCI-DSSv3.1|10.4,PCI-DSSv3.2|10.4,PCI-DSS|10.4,PCI-DSS|10.4.1,PCI-DSS|10.4.3"
          see_also        : "https://workbench.cisecurity.org/files/10"
          value_type      : TIME_MINUTE
          value_data      : 5
          kerberos_policy : CLOCK_SYNCHRONIZATION_TOLERANCE
        </custom_item>

        <custom_item>
          type            : KERBEROS_POLICY
          description     : "1.1.12 Maximum lifetime for service ticket"
          info            : "This control defines the maximum number of minutes that a granted session ticket can be used to access a service."
          solution        : "Make sure 'Maximum lifetime for service ticket' is set to a value of 600 minutes."
          reference       : "800-53|IA-5(13),CCE|CCE-8585-2,CSF|PR.AC-1,PCI-DSS|2.2.3"
          see_also        : "https://workbench.cisecurity.org/files/10"
          value_type      : TIME_MINUTE
          value_data      : 600
          kerberos_policy : SERVICE_TICKET_LIFETIME
        </custom_item>

        <custom_item>
          type            : KERBEROS_POLICY
          description     : "1.1.13 Maximum lifetime for user ticket renewal"
          info            : "This control defines the number of days during which a user`s ticket-grating ticket (TGT) can be renewed."
          solution        : "Make sure 'Maximum lifetime for user ticket renewal' is set to a value of 7 days."
          reference       : "800-53|IA-5(13),CCE|CCE-8000-2,CSF|PR.AC-1,PCI-DSS|2.2.3"
          see_also        : "https://workbench.cisecurity.org/files/10"
          value_type      : TIME_DAY
          value_data      : 7
          kerberos_policy : USER_TICKET_RENEWAL_LIFETIME
        </custom_item>

        <custom_item>
          type            : KERBEROS_POLICY
          description     : "1.1.14 Maximum lifetime for user ticket"
          info            : "This control defines the maximum number of hours a user`s ticket-grating ticket (TGT) may be used."
          solution        : "Make sure 'Maximum lifetime for user ticket' is set to a value of 10 hours."
          reference       : "800-53|IA-5(13),CCE|CCE-8409-5,CSF|PR.AC-1,PCI-DSS|2.2.3"
          see_also        : "https://workbench.cisecurity.org/files/10"
          value_type      : TIME_HOUR
          value_data      : 10
          kerberos_policy : USER_TICKET_LIFETIME
        </custom_item>

        <custom_item>
          type                     : AUDIT_POLICY_SUBCATEGORY
          description              : "1.3.18 Audit Policy: DS Access: Directory Service Access - Domain Controller"
          info                     : "This control defines whether the audit is activated when an AD DS object is accessed."
          solution                 : "Make sure 'Audit Policy: DS Access: Directory Service Access' is set to Success and Failure."
          reference                : "800-171|3.3.1,800-171|3.3.2,800-53|AU-12,CCE|CCE-1926-5,CCE|CCE-2367-1,CN-L3|7.1.3.3(a),CN-L3|7.1.3.3(b),CN-L3|7.1.3.3(c),CN-L3|8.1.3.5(a),CN-L3|8.1.3.5(b),CN-L3|8.1.4.3(a),CSF|DE.CM-1,CSF|DE.CM-3,CSF|DE.CM-7,CSF|PR.PT-1,ISO/IEC-27001|A.12.4.1,ITSG-33|AU-12,NESA|T3.6.2,NESA|T3.6.5,NESA|T3.6.6,NIAv2|SM8,PCI-DSS|10.2.5,PCI-DSS|10.3,PCI-DSS|10.3.1,PCI-DSS|10.3.2,PCI-DSS|10.3.3,PCI-DSS|10.3.4,PCI-DSS|10.3.5,PCI-DSS|10.3.6,SWIFT-CSCv1|6.4,TBA-FIISB|45.1.1"
          see_also                 : "https://workbench.cisecurity.org/files/10"
          value_type               : AUDIT_SET
          value_data               : "Success, Failure"
          audit_policy_subcategory : "Directory Service Access"
        </custom_item>

        <custom_item>
          type                     : AUDIT_POLICY_SUBCATEGORY
          description              : "1.3.19 Audit Policy: DS Access: Directory Service Changes - Domain Controller"
          info                     : "This control defines whether the audit is activated when changes in Active Directory Domain Services (AD DS) occur."
          solution                 : "Make sure 'Audit Policy: DS Access: Directory Service Changes' is set to Success and Failure."
          reference                : "800-171|3.3.1,800-171|3.3.2,800-53|AU-12,CCE|CCE-2445-5,CCE|CCE-2635-1,CN-L3|7.1.3.3(a),CN-L3|7.1.3.3(b),CN-L3|7.1.3.3(c),CN-L3|8.1.3.5(a),CN-L3|8.1.3.5(b),CN-L3|8.1.4.3(a),CSF|DE.CM-1,CSF|DE.CM-3,CSF|DE.CM-7,CSF|PR.PT-1,ISO/IEC-27001|A.12.4.1,ITSG-33|AU-12,NESA|T3.6.2,NESA|T3.6.5,NESA|T3.6.6,NIAv2|SM8,PCI-DSS|10.2.5,PCI-DSS|10.3,PCI-DSS|10.3.1,PCI-DSS|10.3.2,PCI-DSS|10.3.3,PCI-DSS|10.3.4,PCI-DSS|10.3.5,PCI-DSS|10.3.6,SWIFT-CSCv1|6.4,TBA-FIISB|45.1.1"
          see_also                 : "https://workbench.cisecurity.org/files/10"
          value_type               : AUDIT_SET
          value_data               : "Success, Failure"
          audit_policy_subcategory : "Directory Service Changes"
        </custom_item>

        <custom_item>
          type        : USER_RIGHTS_POLICY
          description : "1.8.1 Access this computer from the network - Domain Controller"
          info        : "This control defines whether other users on the network are allowed to connect to this computer."
          solution    : "Make sure 'Access this computer from the network' is set to Administrators, Authenticated Users, and Enterprise Domain Controllers."
