      see_also       : "https://www.bsi.bund.de/cae/servlet/contentblob/471430/publicationFile/28223/standard_100-2_e_pdf.pdf"
      value_type     : POLICY_DWORD
      value_data     : [MIN..3]
      lockout_policy : LOCKOUT_THRESHOLD
    </custom_item>

    <custom_item>
      type           : LOCKOUT_POLICY
      description    : "BSI-100-2: S 4.48: Password protection under Windows systems: Account lockout duration = 60"
      info           : "Account lockout duration (60)

Safeguard Catalogues: S 4: Hardware and software

S 4.48: Password protection under Windows systems"
      reference      : "800-171|3.1.8,800-53|AC-7,BSI-100-2|S4.48,CSCv6|16.7,ITSG-33|AC-7,NESA|T5.5.1,NIAv2|AM24,TBA-FIISB|45.1.2,TBA-FIISB|45.2.1,TBA-FIISB|45.2.2"
      see_also       : "https://www.bsi.bund.de/cae/servlet/contentblob/471430/publicationFile/28223/standard_100-2_e_pdf.pdf"
      value_type     : TIME_MINUTE
      value_data     : [60..MAX]
      lockout_policy : LOCKOUT_DURATION
    </custom_item>
  </then>

  <else>
    <report type:"FAILED">
      description : "BSI-100-2: S 4.48: Password protection under Windows systems: Account lockout threshold = 3"
      info        : "Account lockout duration (60)

Safeguard Catalogues: S 4: Hardware and software

S 4.48: Password protection under Windows systems"
      reference   : "800-171|3.1.8,800-53|AC-7,BSI-100-2|S4.48,CSCv6|16.7,ITSG-33|AC-7,NESA|T5.5.1,NIAv2|AM24,TBA-FIISB|45.1.2,TBA-FIISB|45.2.1,TBA-FIISB|45.2.2"
      see_also    : "https://www.bsi.bund.de/cae/servlet/contentblob/471430/publicationFile/28223/standard_100-2_e_pdf.pdf"
    </report>

    <report type:"FAILED">
      description : "BSI-100-2: S 4.48: Password protection under Windows systems: Account lockout duration = 60"
      info        : "Account lockout duration (60)

Safeguard Catalogues: S 4: Hardware and software

