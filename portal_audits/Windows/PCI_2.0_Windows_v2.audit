<check_type:"Windows" version:"2">
<group_policy:"PCI Windows Audit">

<if>
  <condition type:"AND">
    <custom_item>
      type        : REGISTRY_SETTING
      description : "Check for OS type"
      value_type  : POLICY_TEXT
      value_data  : "Windows (XP|Vista|7)"
      reg_key     : "HKLM\SOFTWARE\Microsoft\Windows NT\CurrentVersion"
      reg_item    : "ProductName"
      check_type  : CHECK_REGEX
    </custom_item>
  </condition>

  <then>
    <custom_item>
      type          : WMI_POLICY
      description   : "PCI 1.4.a Verify that mobile and/or employee-owned computers have a personal firewall installed (root\SecurityCenter)"
      value_type    : POLICY_TEXT
      wmi_namespace : "root\SecurityCenter"
      wmi_request   : "SELECT displayName FROM FirewallProduct"
      wmi_attribute : "displayName"
      wmi_key       : "displayName"
      value_data    : ".+"
      check_type    : CHECK_REGEX
      reference     : "800-171|3.13.1,800-53|SC-7,ITSG-33|SC-7,NIAv2|AM38,NIAv2|SS13d,NIAv2|SS26,PCI-DSS-2.0|1.4"
      see_also      : "https://www.pcisecuritystandards.org/documents/pci_dss_v2.pdf"
      info          : "Note : This check is only applicable to desktop OS's such as Windows XP, Vista, and 7"
    </custom_item>

    <custom_item>
      type          : WMI_POLICY
      description   : "PCI 1.4.a Verify that mobile and/or employee-owned computers have a personal firewall installed (root\SecurityCenter2)"
      value_type    : POLICY_TEXT
      wmi_namespace : "root\SecurityCenter2"
      wmi_request   : "SELECT displayName FROM FirewallProduct"
      wmi_attribute : "displayName"
      wmi_key       : "displayName"
      value_data    : ".+"
      check_type    : CHECK_REGEX
      reference     : "800-171|3.13.1,800-53|SC-7,ITSG-33|SC-7,NIAv2|AM38,NIAv2|SS13d,NIAv2|SS26,PCI-DSS-2.0|1.4"
      see_also      : "https://www.pcisecuritystandards.org/documents/pci_dss_v2.pdf"
      info          : "Note : This check is only applicable to desktop OS's such as Windows XP, Vista, and 7"
    </custom_item>

    <custom_item>
      type          : WMI_POLICY
      description   : "PCI 5.1 Deploy anti-virus software on all systems commonly affected by malicious software (root\SecurityCenter)"
      value_type    : POLICY_TEXT
      wmi_namespace : "root\SecurityCenter"
      wmi_request   : "SELECT displayName FROM AntiVirusProduct"
      wmi_attribute : "displayName"
      wmi_key       : "displayName"
      value_data    : ".+"
      reference     : "800-171|3.14.2,800-171|3.14.4,800-171|3.14.5,800-53|SI-3,CN-L3|7.1.3.6(b),CSF|DE.CM-4,CSF|DE.DP-3,ISO/IEC-27001|A.12.2.1,ITSG-33|SI-3,NIAv2|GS8a,PCI-DSS-2.0|5.1"
      check_type    : CHECK_REGEX
      see_also      : "https://www.pcisecuritystandards.org/documents/pci_dss_v2.pdf"
      info          : "Note this check is only applicable to desktop OS's such as Windows XP, Vista, and 7"
    </custom_item>

    <custom_item>
      type          : WMI_POLICY
      description   : "PCI 5.1 Deploy anti-virus software on all systems commonly affected by malicious software (root\SecurityCenter2)"
      value_type    : POLICY_TEXT
      wmi_namespace : "root\SecurityCenter2"
      wmi_request   : "SELECT displayName FROM AntiVirusProduct"
      wmi_attribute : "displayName"
      wmi_key       : "displayName"
      value_data    : ".+"
      reference     : "PCI-DSS-2.0|5.1"
      check_type    : CHECK_REGEX
      see_also      : "https://www.pcisecuritystandards.org/documents/pci_dss_v2.pdf"
      info          : "Note this check is only applicable to desktop OS's such as Windows XP, Vista, and 7"
    </custom_item>

    <custom_item>
      type          : WMI_POLICY
      description   : "PCI 5.2 Ensure that all anti-virus mechanisms are current, actively running, and generating audit logs (WMI)"
      value_type    : POLICY_TEXT
      wmi_namespace : "root\SecurityCenter"
      wmi_request   : "SELECT productUptoDate FROM AntiVirusProduct"
      wmi_attribute : "productUptoDate"
      wmi_key       : "productUptoDate"
      value_data    : "[Yy][Ee][Ss]"
      check_type    : CHECK_REGEX
      reference     : "PCI-DSS-2.0|5.2"
      see_also      : "https://www.pcisecuritystandards.org/documents/pci_dss_v2.pdf"
      info          : "Note this check is only applicable to desktop OS's such as Windows XP, Vista, and 7"
    </custom_item>
  </then>
</if>

<custom_item>
  type        : REGISTRY_SETTING
  description : "PCI 1.4.a Verify that mobile and/or employee-owned computers have a personal firewall installed (standard profile registry check)"
  value_type  : POLICY_DWORD
  value_data  : 1
  reg_key     : "HKLM\SOFTWARE\Policies\Microsoft\WindowsFirewall\StandardProfile"
  reg_item    : "EnableFirewall"
  reference   : "800-171|3.13.1,800-53|SC-7,CSCv6|9.2,ITSG-33|SC-7,NIAv2|AM38,NIAv2|SS13d,NIAv2|SS26,PCI-DSS-2.0|1.4"
  reg_type    : REG_DWORD
  reg_option  : CAN_BE_NULL
</custom_item>

