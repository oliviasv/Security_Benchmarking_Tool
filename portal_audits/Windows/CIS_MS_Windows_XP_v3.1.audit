      see_also    : "https://workbench.cisecurity.org/files/1547"
      value_type  : POLICY_TEXT
# Note: Variable @LEGAL_NOTICE_TEXT@ replaced with "All activities performed on this system will be monitored." in field "value_data".
      value_data  : "All activities performed on this system will be monitored."
