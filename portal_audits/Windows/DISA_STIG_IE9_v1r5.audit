      value_type  : POLICY_DWORD
      value_data  : 1
      reg_key     : "HKLM\Software\Microsoft\Windows\CurrentVersion\Policies\Ext"
      reg_item    : "NoFirsttimeprompt"
      reg_option  : CAN_NOT_BE_NULL
    </custom_item>

