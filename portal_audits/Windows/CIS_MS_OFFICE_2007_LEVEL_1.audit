<check_type:"Windows" version:"2">
<group_policy:"CIS MS Office 2007: Level I">

<if>
  <condition type:"AND">
    <custom_item>
      type        : REGISTRY_SETTING
      description : "Office 2007 is installed"
      value_type  : POLICY_DWORD
      value_data  : [1..MAX]
      reg_key     : "HKLM\SOFTWARE\Microsoft\Office\12.0\Common\InstallRoot"
      reg_item    : "InstallCount"
      reg_option  : CAN_NOT_BE_NULL
    </custom_item>
  </condition>

  <then>
    <report type:"PASSED">
      description : "CIS_MS_OFFICE_2007_LEVEL_1.audit Level 1"
    </report>

# 1.1.1.1. Office Updates: Level I

    <custom_item>
      type                 : REGISTRY_SETTING
      description          : "1.1.1.1 Office Updates: Level I disabled"
      info                 : "This setting determines if a User may acquire updates from the Microsoft Office Update site"
      solution             : "Make sure 'Office Updates: Level I' is disabled."
      reference            : "800-53|CM-7,800-53|SI-2,800-53|SI-3,CCE|CCE-784-9,LEVEL|1S,PCI-DSS|2.2.4,PCI-DSS|6.1"
      see_also             : "https://workbench.cisecurity.org/files/618"
      value_type           : POLICY_DWORD
      value_data           : 0
      reg_key              : "HKU\Software\Policies\Microsoft\Office\Common\OfficeUpdate"
      reg_item             : "BlockUpdates"
      reg_ignore_hku_users : "S-1-5-18,S-1-5-19,S-1-5-20"
      reg_option           : CAN_NOT_BE_NULL
    </custom_item>

