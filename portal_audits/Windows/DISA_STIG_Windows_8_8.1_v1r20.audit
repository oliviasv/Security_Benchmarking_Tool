      reg_key     : "HKLM\Software\Policies\Microsoft\Windows Nt\Terminal Services"
      reg_item    : "PerSessionTempDir"
      reg_option  : CAN_NOT_BE_NULL
    </custom_item>

    <custom_item>
      type        : REGISTRY_SETTING
      description : "WN08-CC-000103 - Remote Desktop Services must delete temporary folders when a session is terminated."
      info        : "Remote desktop session temporary folders must always be deleted after a session is over to prevent hard disk clutter and potential leakage of information.  This setting controls the deletion of the temporary folders when the session is terminated."
      solution    : "Configure the policy value for Computer Configuration -> Administrative Templates -> Windows Components -> Remote Desktop Services -> Remote Desktop Session Host -> Temporary Folders -> 'Do not delete temp folder upon exit' to 'Disabled'."
