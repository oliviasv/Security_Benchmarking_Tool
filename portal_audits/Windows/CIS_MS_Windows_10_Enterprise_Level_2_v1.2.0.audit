      reg_key     : "HKLM\System\Currentcontrolset\Services\Bthhfsrv"
      reg_item    : "Start"
    </custom_item>

    <custom_item>
      type        : REGISTRY_SETTING
      description : "5.2 (L2) Ensure 'Bluetooth Support Service (bthserv)' is set to 'Disabled'"
