      see_also    : "https://workbench.cisecurity.org/files/1721"
      value_type  : POLICY_DWORD
      value_data  : 1
      reg_key     : "HKLM\Software\Policies\Microsoft\Windows\Internet Connection Wizard"
      reg_item    : "ExitOnMSICW"
    </custom_item>

